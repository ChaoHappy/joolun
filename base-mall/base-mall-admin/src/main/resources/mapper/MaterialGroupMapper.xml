<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.joolun.cloud.mall.admin.mapper.MaterialMapperGroup">

	<resultMap id="materialGroupMap" type="com.joolun.cloud.mall.common.entity.MaterialGroup">
		<id property="id" column="id"/>
		<result property="tenantId" column="tenant_id"/>
		<result property="delFlag" column="del_flag"/>
		<result property="createTime" column="create_time"/>
		<result property="updateTime" column="update_time"/>
		<result property="createId" column="create_id"/>
		<result property="name" column="name"/>
	</resultMap>
	<select id="selectMaterialGroup" parameterType="com.joolun.cloud.mall.common.entity.Material" resultType="com.joolun.cloud.mall.common.entity.Material">
		SELECT id, tenant_id, del_flag, create_time, update_time, create_id, name FROM material_group WHERE material_group.tenant_id = #{tenantId}
	</select>
	<select id="selectMaterialGroupById" parameterType="java.lang.String" resultType="com.joolun.cloud.mall.common.entity.Material">
		SELECT id, tenant_id, del_flag, create_time, update_time, create_id, name FROM material_group WHERE material_group.tenant_id = #{tenantId} ORDER BY create_time DESC
	</select>
</mapper>
